<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class= "DXMauiApp1.Pages.ContactCreatePage"
             Title="ContactCreatePage">
    <ContentPage.Content>
        <VerticalStackLayout 
            Margin="24"
            Spacing="24">

            <Button 
                x:Name="ButtonSubmit"
                Clicked="ButtonSubmit_Clicked"
                CornerRadius="0"
                FontSize="18"
                HeightRequest="56"
                Style="{StaticResource PrimaryButton}"
                Text="匚匚匚匚" />

            <Button 
                x:Name="ButtonCancel"
                Clicked="ButtonCancel_Clicked"
                CornerRadius="0"
                FontSize="18"
                HeightRequest="56"
                Style="{StaticResource PrimaryButton}"
                Text="匚匚" />

        </VerticalStackLayout>
    </ContentPage.Content>
</ContentPage>